import random

tries=0
number=0
answer=random.randint(1,100)
print("1부터 100까지 숫자중 한개를 맞추시오")
while number!=answer and tries<=10:
    number=int(input("숫자를 입력:"))
    tries= tries+1
    if number<answer:
        print("낮음")
        
    elif number>answer:
        print("높음")

print("축하함. 당신의 시도 횟수는",tries)
    
